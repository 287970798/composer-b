{
	"require":{
		"noahbuscher/macaw": "dev-master",
		"illuminate/database": "*",
		"filp/whoops": "*",
		"nette/mail": "*"
	},
	"repositories": {
		"packagist": {
			"type": "composer",
			"url": "https://packagist.phpcomposer.com"
		}
	},
	"autoload": {
		"classmap": [
			"app/controllers",
			"app/models",
			"services"
		],
		"psr-4": {
			"app\\controllers\\": "app/controllers/",
			"app\\models\\": "app/models/",
			"services\\": "services"
		}
	}
}
